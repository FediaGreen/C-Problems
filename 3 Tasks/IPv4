#include <array>
#include <cstdint>
#include <iostream>
#include <string>

class IPv4 {
 public:
  void UpdateNotation() {
    notation_ = (static_cast<uint32_t>(octets_[0]) << 24) |
                (static_cast<uint32_t>(octets_[1]) << 16) |
                (static_cast<uint32_t>(octets_[2]) << 8) |
                (static_cast<uint32_t>(octets_[3]));
  }

  void Normalise() {
    octets_[0] = uint8_t((notation_ >> 24) & 0xFF);
    octets_[1] = uint8_t((notation_ >> 16) & 0xFF);
    octets_[2] = uint8_t((notation_ >> 8) & 0xFF);
    octets_[3] = uint8_t(notation_ & 0xFF);
  }

  IPv4& operator++() {
    notation_++;
    Normalise();
    return *this;
  }

  IPv4 operator++(int) {
    IPv4 old = *this;
    ++notation_;
    Normalise();
    return old;
  }

  IPv4& operator--() {
    notation_--;
    Normalise();
    return *this;
  }

  IPv4 operator--(int) {
    IPv4 old = *this;
    --notation_;
    Normalise();
    return old;
  }

  friend bool operator<(IPv4& ip1, IPv4& ip2);

  friend std::ostream& operator<<(std::ostream& os, IPv4& ip);
  
  friend std::istream& operator>>(std::istream& in, IPv4& ip);

 private:
  std::array<std::uint8_t, 4> octets_{0, 0, 0, 0};
  uint32_t notation_ = 0;
};

bool operator<(IPv4& ip1, IPv4& ip2) {
  return (ip1.notation_) < (ip2.notation_);
}

std::ostream& operator<<(std::ostream& os, IPv4& ip) {
  for (size_t i = 0; i < ip.octets_.size(); ++i) {
    os << static_cast<int>(ip.octets_[i]);
    if (i + 1 != ip.octets_.size()) {
      os << ".";
    }
  }
  return os;
}

std::istream& operator>>(std::istream& in, IPv4& ip) {
  std::string notation = "";
  in >> notation;
  uint8_t value = 0;
  int point_count = 0;
  size_t ind = 0;
  size_t idx = 0;
  while(point_count < 3 && ind < notation.size()) {
    if (notation[ind] == '.') {
      ip.octets_[idx] = value;
      ++idx;
      value = 0;
      ++point_count;
    } else {
      value = uint8_t(value * 10 + (static_cast<uint8_t>(notation[ind]) - 48));
    }
    ++ind;
  }
  while(ind < notation.size()) {
    value = uint8_t(value * 10 + (static_cast<uint8_t>(notation[ind]) - 48));
    ++ind;
  }
  ip.octets_[idx] = value;
  ip.UpdateNotation();
  return in;
}

int main() {
  IPv4 ip;
  std::cin >> ip;
  std::cout << ip << '\n';
  ip++;
  std::cout << ip << '\n';
  ip--;
  std::cout << ip << '\n'; // same with first
}
